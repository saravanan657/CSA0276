#include<stdio.h>
int main()
{
  char s[50]="SARAVANAN";
  int i=0,vowels=0,consonant=0;
  while(s[i]!='\0')
  {
    if((s[i]>='A'&& s[i]<='Z')||(s[i]>='a'&& s[i]<='z'))
    {
      if(s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U'
||s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u')
        vowels++;
      else
        consonant++;
    }
    i++;
  }
  printf("VOWELS:%d\nCONSONANTS:%d",vowels,consonant);
  return 0;
}
